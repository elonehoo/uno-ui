{
  "name": "@uno-ui/monorepo",
  "private": true,
  "type": "module",
  "version": "0.0.0",
  "packageManager": "pnpm@7.0.0",
  "scripts": {
    "prepare": "pnpm -r run stub",
    "release": "bumpp package.json packages/*/package.json --commit --push --tag && pnpm -r publish --access public",
    "typeCheck": "tsc --noEmit",
    "test": "vitest",
    "coverage": "vitest run --coverage",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "example:dev": "npm -C example/vue run dev",
    "example:build": "npm -C example/vue run build",
    "example:preview": "npm -C example/vue run preview"
  },
  "devDependencies": {
    "@unoui/button": "workspace:*",
    "@elonehoo/eslint-config": "^2.0.0",
    "@iconify-json/carbon": "^1.1.4",
    "@types/node": "^17.0.31",
    "@unocss/reset": "^0.32.13",
    "@vitejs/plugin-vue": "^2.3.2",
    "bumpp": "^7.1.1",
    "eslint": "^8.15.0",
    "pnpm": "^7.0.1",
    "rimraf": "^3.0.2",
    "typescript": "^4.6.4",
    "unbuild": "^0.7.4",
    "unocss": "^0.32.13",
    "vite": "^2.9.8",
    "vitest": "^0.10.5",
    "vue": "^3.2.33",
    "vue-tsc": "^0.34.11"
  }
}
